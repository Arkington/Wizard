/// @param _atk_obj
/// @param _menu_spr
function Attack(_atk_obj, _menu_spr) {
    return {
        atk_obj: _atk_obj,
        menu_spr: _menu_spr,
    }
}

#macro SoulFlame Attack(oAtkSoulFlame, sMenuSoulFlame_tmp)
#macro StunShock Attack(oAtkStunShock, sMenuStunShock_tmp)
#macro Grapes Attack(oAtkGrapes, sMenuGrapes)